<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Abstractions for defining health checks in .NET applications
Commonly Used Types
Jokersoft.Extensions.Diagnostics.HealthChecks.IHealthCheck
    </Description>
    <RootNamespace>Jokersoft.Extensions.Diagnostics.HealthChecks</RootNamespace>
    <TargetFrameworks>$(DefaultNetFxTargetFramework);netstandard2.0;$(DefaultNetCoreTargetFramework)</TargetFrameworks>
    <TargetFrameworks Condition="'$(DotNetBuildFromSource)' == 'true'">$(DefaultNetCoreTargetFramework)</TargetFrameworks>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageTags>diagnostics;healthchecks</PackageTags>
    <IsAspNetCoreApp>true</IsAspNetCoreApp>
    <IsTrimmable>true</IsTrimmable>
  <GeneratePackageOnBuild>True</GeneratePackageOnBuild></PropertyGroup>
</Project>