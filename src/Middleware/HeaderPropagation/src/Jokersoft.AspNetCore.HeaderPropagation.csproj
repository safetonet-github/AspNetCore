<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>ASP.NET Core middleware to propagate HTTP headers from the incoming request to the outgoing HTTP Client requests</Description>
    <TargetFramework>$(DefaultNetCoreTargetFramework)</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageTags>aspnetcore;httpclient</PackageTags>
    <IsTrimmable>true</IsTrimmable>
  <GeneratePackageOnBuild>True</GeneratePackageOnBuild></PropertyGroup>
  <ItemGroup>
    <InternalsVisibleTo Include="Jokersoft.AspNetCore.HeaderPropagation.Tests" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Jokersoft.AspNetCore.Http" />
    <Reference Include="Microsoft.Extensions.Http" />
    <Reference Include="Microsoft.Extensions.DependencyInjection" />
  </ItemGroup>
</Project>