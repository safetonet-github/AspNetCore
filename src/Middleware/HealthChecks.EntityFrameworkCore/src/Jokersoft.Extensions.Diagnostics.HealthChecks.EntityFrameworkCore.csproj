<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>
      Components for performing health checks using EntityFrameworkCore.
    </Description>
    <TargetFramework>$(DefaultNetCoreTargetFramework)</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageTags>diagnostics;healthchecks;entityframeworkcore</PackageTags>
    <BaseNamespace>Jokersoft.Extensions.Diagnostics.HealthChecks</BaseNamespace>
    <IsTrimmable>true</IsTrimmable>
  <GeneratePackageOnBuild>True</GeneratePackageOnBuild></PropertyGroup>
  <ItemGroup>
    <Reference Include="Microsoft.EntityFrameworkCore.Relational" />
    <Reference Include="Jokersoft.Extensions.Diagnostics.HealthChecks.Abstractions" />
    <Reference Include="Jokersoft.Extensions.Diagnostics.HealthChecks" />
  </ItemGroup>
</Project>